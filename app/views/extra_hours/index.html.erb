<p id="notice"><%= notice %></p>

<h1>Extra Hours</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>Execution date</th>
      <th>Profile</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @extra_hours.each do |extra_hour| %>
      <tr>
        <td><%= extra_hour.project %></td>
        <td><%= extra_hour.execution_date %></td>
        <td><%= extra_hour.profile %></td>
        <td><%= extra_hour.status %></td>
        <td><%= link_to 'Show', extra_hour %></td>
        <td><%= link_to 'Edit', edit_extra_hour_path(extra_hour) %></td>
        <td><%= link_to 'Destroy', extra_hour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Extra Hour', new_extra_hour_path %>
