<p id="notice"><%= notice %></p>

<h1>Voltage Levels</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @voltage_levels.each do |voltage_level| %>
      <tr>
        <td><%= voltage_level.name %></td>
        <td><%= link_to 'Show', voltage_level %></td>
        <td><%= link_to 'Edit', edit_voltage_level_path(voltage_level) %></td>
        <td><%= link_to 'Destroy', voltage_level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Voltage Level', new_voltage_level_path %>
